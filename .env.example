# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Clerk Authentication
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key

# Optional: Sentry Error Monitoring
VITE_SENTRY_DSN=your_sentry_dsn

# Optional: Analytics
VITE_POSTHOG_KEY=your_posthog_api_key

# Optional: Email Service (if using Resend instead of Supabase)
VITE_RESEND_API_KEY=your_resend_api_key

# Environment
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE=true
VITE_ENABLE_AI_ASSISTANT=false

# API Configuration
VITE_API_TIMEOUT=30000
VITE_MAX_RETRY_ATTEMPTS=3

# Storage Limits
VITE_MAX_IMAGE_SIZE_MB=5
VITE_MAX_ATTACHMENT_SIZE_MB=10

# Compliance Settings
VITE_DATA_RETENTION_DAYS=365
VITE_TEMPERATURE_ALERT_THRESHOLD_MINUTES=30