{
  "git": {
    "deploymentEnabled": {
      "main-precompilato": true
    }
  },
  "buildCommand": "NODE_ENV=development npm run build",
  "outputDirectory": "docs",
  "env": {
    "NODE_ENV": "development"
  },
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    }
  ]
} 